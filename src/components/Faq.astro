---
import { faqsData } from '../data/faqsData.js';
import FaqSwiper from './react/FaqSwiper.jsx';

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<section class="relative m-auto pb-12 w-full max-w-screen-xl lg:w-[90%]">
  <FaqSwiper client:load faqs={faqsData} title={title} />
</section>